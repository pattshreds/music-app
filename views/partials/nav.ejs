<a href="/playlist">
  <img src="/images/moodsetlogo.png" class="brand-logo left" />
</a>
<ul class="right">
  <li><a href="/playlist/new">Create Playlist</a></li>
  <% if (currentUser) { %>
    <li class="welcome">Welcome, <%= currentUser.username %></li>
    <li>
      <form action="/sessions?_method=DELETE" method="post">
        <input type="submit" value="Log Out" class="btn-small">
      </form>
    </li>
  <% } else { %>
    <li><a href="/users/new">Sign Up</a> </li>
    <li><a href="/sessions/new">Log In</a> </li>
  <% } %>
</ul>
